apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: kapp-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /home
spec:
  rules:
  - host: advert.test
    http:
      paths:
      - path: /home
        backend: 
          serviceName: kapp
          servicePort: 80
