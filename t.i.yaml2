apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: nginx
    #ingress.kubernetes.io/ssl-redirect: "false"
    ingress.kubernetes.io/rewrite-target: /
  labels:
    app:  kapp 
  name:  kapp 
spec:
  backend:
    serviceName:  kapp 
    servicePort: 80
  rules: 
  - host: advent.test 
    http:
      paths:
      - path: /
        backend: 
          serviceName:  kapp 
          servicePort: 80
